{
  "name": "@joj/adt",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "description": "Functional Algebratic Types",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-flow": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "babel-eslint": "^8.2.3",
    "chai": "4.1.0",
    "eslint": "^4.18.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "eslint-plugin-fp": "^2.3.0",
    "flow-bin": "^0.59.0",
    "flow-typed": "^2.4.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.11.0"
  },
  "scripts": {
    "lint": "eslint src/",
    "lint-fix": "eslint src/ --fix",
    "prepublish": "npm run build",
    "test": "flow && mocha --check-leaks --recursive --require test/testSetup test/**/*Spec.js",
    "test-dev": "nodemon --watch src/ --watch test/ --exec 'mocha --recursive --require test/testSetup test/**/*Spec.js || true'",
    "flow": "flow",
    "build": "rm -rf dist && babel src --out-dir dist --copy-files && cp -f package.json dist",
    "flow-init": "flow init",
    "flow-status": "flow status"
  }
}
